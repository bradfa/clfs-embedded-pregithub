+		write:	mini_fo_write,
+		readdir: mini_fo_readdir,
+		poll:	mini_fo_poll,
+		/* ioctl:	mini_fo_ioctl, */
+		mmap:	mini_fo_mmap,
+		open:	mini_fo_open,
+		flush:	mini_fo_flush,
+		release: mini_fo_release,
+		fsync:	mini_fo_fsync,
+		fasync:	mini_fo_fasync,
